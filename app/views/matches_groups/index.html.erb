<div class="page-header">
  <h1>
    Matches groups
    <a href="/matches_groups/new" class="btn btn-lg btn-success">New Matches group</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Team one</th>
        <th>Team two</th>
        <th>Date</th>
        <th>Location</th>
        <th>Groupletter</th>
        <th>City</th>
        <th>Goals one</th>
        <th>Goals two</th>
        <th>Winner</th>
        <th>Actions</th>
      </tr>

      <% @matches_groups.each do |matches_group| %>
      <tr>
        <td><%= matches_group.team_one %></td>
        <td><%= matches_group.team_two %></td>
        <td><%= matches_group.date %></td>
        <td><%= matches_group.location %></td>
        <td><%= matches_group.groupletter %></td>
        <td><%= matches_group.city %></td>
        <td><%= matches_group.goals_one %></td>
        <td><%= matches_group.goals_two %></td>
        <td><%= matches_group.winner %></td>
        <td>
          <a href="/matches_groups/<%= matches_group.id %>" class="btn btn-primary">Show</a>
          <a href="/matches_groups/<%= matches_group.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_matches_group/<%= matches_group.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
